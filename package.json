    "babel-loader": "^8.0.4",
    "babel-plugin-emotion": "^10.0.2",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.21",
