{
  "name": "kysley-react-4",
  "version": "1.0.0",
  "description": "4th Revsion of Evan Kysley's personal portfolio",
  "main": "",
  "keywords": [],
  "author": "Evan Kysley",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --mode development --hot --config webpack.dev.config.babel.js",
    "start:new": "cross-env NODE_ENV=development webpack-serve --require @babel/register --config webpack.dev.config.babel.js",
    "build": "cross-env NODE_ENV=production webpack --mode production --progress --config webpack.prod.config.babel.js",
    "test": "jest --coverage --env=jsdom",
    "test:watch": "jest --coverage --env=jsdom --watch"
  },
